<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>com.beenthere.refresh</string>
		<string>com.beenthere.processing</string>
		<string>com.beenthere.dailyexport</string>
	</array>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
	<key>NSCameraUsageDescription</key>
	<string>Been There uses the camera to scan QR codes from PhoneTrack for easy configuration.</string>
	<key>NSFaceIDUsageDescription</key>
	<string>Been There uses Face ID to secure and protect your travel data from unauthorized access.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Been There needs continuous location access to send your position to Nextcloud PhoneTrack, even when the app is in the background.</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>Been There needs always-on location access to track your position continuously and send it to Nextcloud PhoneTrack.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Been There needs location access to send your position to Nextcloud PhoneTrack.</string>
	<key>NSMotionUsageDescription</key>
	<string>Been There uses motion data to detect when you're moving and adjust tracking frequency accordingly to save battery.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Been There can import location data from your photos to automatically discover places you've visited. Your photos stay on your device - only location metadata is extracted.</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>location</string>
		<string>fetch</string>
		<string>processing</string>
	</array>
	<key>UILaunchScreen</key>
	<dict>
		<key>UIColorName</key>
		<string>SplashBackground</string>
	</dict>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
</dict>
</plist>
